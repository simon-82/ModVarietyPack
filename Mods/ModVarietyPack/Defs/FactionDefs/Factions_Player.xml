      <li>Urban</li>
    </hairTags>
    <apparelStuffFilter>
      <thingDefs>
        <li>Synthread</li>
      </thingDefs>
    </apparelStuffFilter>
  </FactionDef>
  
  <FactionDef Parent="PlayerFactionBase">
    <defName>PlayerTribe</defName>
    <label>tribe</label>
    <description>Your own tribe.</description>
    <isPlayer>true</isPlayer>
    <pawnsPlural>tribespeople</pawnsPlural>
    <basicMemberKind>Tribesperson</basicMemberKind>
    <factionNameMaker>NamerFactionTribal</factionNameMaker>
    <techLevel>Neolithic</techLevel>
    <backstoryCategory>Tribal</backstoryCategory>
    <pawnNameMaker>NamerPersonTribal</pawnNameMaker>
    <!--  <startingResearchTags>
      <li>TribalStart</li>
    </startingResearchTags> -->
    <hairTags>
      <li>Tribal</li>
    </hairTags>
    <apparelStuffFilter>
      <thingDefs>
        <li>Cloth</li>
      </thingDefs>
    </apparelStuffFilter>
    </FactionDef>

</Defs>